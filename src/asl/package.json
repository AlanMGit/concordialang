{
  "name": "asl",
  "description": "Agile Specification Language Interpreter",
  "keywords": [
    "agile",
    "specification",
    "language",
    "verification",
    "validation",
    "test"
  ],
  "license": "AGPL",
  "version": "0.1.0",
  "readme": "README.md",
  "homepage": "https://site.org",
  "repository": {
    "type": "git",
    "url": "git://gitlab.com/thiagodp/asl.git"
  },
  "bugs": {
    "url": "https://gitlab.com/thiagodp/asl/issues"
  },
  "author": {
    "name": "Thiago Delgado Pinto",
    "email": "email@site.org",
    "url": "https://site.org"
  },
  "main": "main.js",
  "allow-scripts": true,
  "scripts": {
    "build": "tsc",
    "test": "jest"
  },
  "dependencies": {
    "meow": "^3.7.0",
    "node-glob": "^1.2.0",
    "ora": "^1.3.0"
  },
  "devDependencies": {
    "typescript": "^2.2.1",
    "@types/core-js": "^0.9.42",
    "@types/node": "^8.0.9",
    "@types/jest": "^18.1.1",
    "@types/nightmare": "^1.6.30",
    "jest": "^19.0.2",
    "ts-jest": "^19.0.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
