    "he": "^1.1.1",
    "is-my-json-valid": "^2.16.1",
    "js-joda": "^1.5.2",
    "json-schema-generator": "^2.0.3",
    "memfs": "^2.5.7",
    "meow": "^3.7.0",
    "mustache": "^2.3.0",
    "node-cmd": "^3.0.0",
    "node-glob": "^1.2.0",
    "ora": "^1.3.0",
    "randexp": "^0.4.5",
    "xregexp": "^3.2.0"
  },
  "devDependencies": {
    "@types/chalk": "^0.4.31",
    "@types/core-js": "^0.9.42",
    "@types/es6-promise": "0.0.33",
    "@types/glob": "^5.0.32",
    "@types/jest": "^18.1.1",
    "@types/meow": "^3.6.2",
    "@types/nightmare": "^1.6.30",
    "@types/node": "^8.0.14",
    "@types/ora": "^1.3.1",
    "@types/whatwg-streams": "0.0.3",
    "jest": "^21.1.0",
    "mem-fs": "^1.1.3",
    "ts-jest": "^21.0.1",
    "typescript": "^2.4.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "./node_modules/ts-jest/preprocessor.js"
