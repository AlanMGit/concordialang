{
  "name": "concordia",
  "description": "Concordia Language Compiler",
  "keywords": [
    "concordia",
    "agile",
    "specification",
    "language",
    "test",
    "verification",
    "validation"
  ],
  "license": "AGPL-3.0",
  "version": "0.32.0",
  "readme": "README.md",
  "homepage": "https://site.org",
  "repository": {
