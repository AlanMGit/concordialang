{
  "name": "concordia",
  "description": "Concordia Language Compiler",
  "keywords": [
    "concordia",
    "agile",
    "specification",
    "language",
    "test",
    "verification",
    "validation"
  ],
  "license": "AGPL-3.0",
  "version": "0.52.0",
  "readme": "README.md",
  "homepage": "https://site.org",
  "repository": {
    "type": "git",
    "url": "git://gitlab.com/thiagodp/concordialang.git"
  },
  "bugs": {
    "url": "https://gitlab.com/thiagodp/concordialang/issues"
  },
  "author": {
    "name": "Thiago Delgado Pinto",
    "email": "email@site.org",
    "url": "https://site.org"
  },
  "main": "./dist/main.js",
  "bin": {
    "concordia": "./dist/main.js"
  },
  "allow-scripts": true,
  "scripts": {
    "build": "tsc",
    "test": "node copy.js && jest --all",
    "start": "node copy.js && node bin/concordia"
  },
  "dependencies": {
    "alasql": "^0.4.5",
    "chalk": "^2.3.0",
    "cli-truncate": "^1.0.0",
    "database-js": "^3.0.2",
    "database-js-json": "^1.2.0",
    "dedent-js": "^1.0.1",
    "figures": "^2.0.0",
    "filewalker": "^0.1.3",
    "graph.js": "^1.21.1",
    "he": "^1.1.1",
    "is-my-json-valid": "^2.16.1",
    "js-joda": "^1.5.2",
    "json-schema-generator": "^2.0.3",
    "log-update": "^2.3.0",
    "meow": "^3.7.0",
    "moment": "^2.19.3",
    "mustache": "^2.3.0",
    "node-fs-extra": "^0.8.2",
    "node-glob": "^1.2.0",
    "ora": "^1.3.0",
    "pretty-bytes": "^4.0.2",
    "randexp": "^0.4.5",
    "seedrandom": "^2.4.3",
    "sprintf-js": "^1.1.1",
    "sqlstring": "^2.3.0",
    "xregexp": "^3.2.0"
  },
  "devDependencies": {
    "@types/chalk": "^0.4.31",
    "@types/core-js": "^0.9.46",
    "@types/glob": "^5.0.32",
    "@types/jest": "^21.1.5",
    "@types/meow": "^3.6.2",
    "@types/nightmare": "^1.6.30",
    "@types/node": "^8.0.14",
    "@types/ora": "^1.3.1",
    "jest": "^21.1.0",
    "memfs": "^2.5.7",
    "ts-jest": "^21.0.1",
    "typescript": "^2.4.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "./node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
