# This project uses EditorConfig (http://editorconfig.org) to standardize
# tabulation, charset and other attributes of source files. Please head to
# http://editorconfig.org/#download to check if your editor comes with
# EditorConfig bundled or to download a plugin.

root = true # Top-most EditorConfig file

[*] # Applies rules for all files under the current directory and its subdirectories
insert_final_newline = true # Empty line at the end of file
charset = utf-8 # Default charset is UTF-8
indent_style = space # Indentation with spaces
indent_size = 4 # Indentaion size of 4 (spaces)
